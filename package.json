	},
	"license": "MIT",
	"engines": {
		"node": ">=20"
	},
	"funding": {
		"type": "custom",
		"url": "https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=JVZVXBC4N9DAN"
	},
	"devDependencies": {
